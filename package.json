{
  "name": "foodprint",
  "version": "1.0.0",
  "description": "Food supply chain on Blockchain for small-scale farmers",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "^18.x",
    "npm": "^9.x"
  },
  "scripts": {
    "bundle": "browserify src/js/app.js | uglifyjs -mc > src/js/bundle.js",
    "test": "NODE_ENV=test mocha --timeout 30000 tests/api/*.js",
    "testdb": "node tests/sequelize.js",
    "insertproduce": "node tests/produce.js",
    "inserttestsupplierproduce": "node tests/test_supplier_produce_scan.js",
    "build-dev": "sequelize db:migrate --env development",
    "build": "sequelize db:migrate --env production",
    "build-dev-undo": "sequelize db:migrate:undo --env development",
    "heroku-postbuild": "sequelize db:migrate --env production && npm run bundle",
    "dev": "nodemon server.js --host 0.0.0.0",
    "start": "node server.js",
    "format": "prettier --write \"README.md\" \"server.js\" \"utils/**/*.js\" \"migrations/**/*.js\" \"routes/**/*.js\" \"models/**/*.js\" \"config/**/*.js\"",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "author": "Julian Kanjere, FoodPrint Labs",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/express-session": "^1.17.8",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "browser-sync": "^1.3.7",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-notify": "^5.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.0.3",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.6.0",
    "watchify": "^4.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.775.0",
    "@aws-sdk/credential-providers": "^3.775.0",
    "@aws-sdk/lib-storage": "^3.775.0",
    "algosdk": "2.8.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "browserify": "^17.0.0",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "connect-session-sequelize": "^7.1.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.9",
    "express": "^4.21.2",
    "express-flash": "^0.0.2",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "heroku-ssl-redirect": "^0.0.4",
    "http-errors": "^2.0.0",
    "jimp": "^0.22.8",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.0",
    "node-fetch": "^3.3.1",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.1",
    "qrcode": "^1.5.0",
    "rate-limit-redis": "^3.0.0",
    "redis": "^4.6.13",
    "sequelize": "^6.37.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^4.0.0",
    "uglify-js": "^3.17.4",
    "uuid": "^8.3.2",
    "winston": "^3.11.0"
  },
  "repository": "git@github.com:kinyuagk/foodprint.git",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "tabWidth": 2,
    "semi": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "prettier"
    ],
    "rules": {
      "node/no-unsupported-features/es-syntax": "off"
    },
    "env": {
      "node": true,
      "es6": true
    }
  }
}